      "source": [
        "import streamlit as st\n",
        "import folium\n",
        "from streamlit_folium import st_folium\n",
        "\n",
        "st.title(\"Map Chat App\")\n",
        "\n",
        "# Initial map location (example: Johannesburg)\n",
        "m = folium.Map(location=[-26.2041, 28.0473], zoom_start=6)\n",
        "\n",
        "# Chat memory state\n",
        "if 'chat_history' not in st.session_state:\n",
        "    st.session_state['chat_history'] = []\n",
        "\n",
        "def handle_chat(input_text):\n",
        "    # Example: Add marker if location in text\n",
        "    if 'add marker' in input_text.lower():\n",
        "        # Simple location parsing\n",
        "        if 'cape town' in input_text.lower():\n",
        "            folium.Marker(location=[-33.9249, 18.4241], popup='Cape Town').add_to(m)\n",
        "            return \"Added marker for Cape Town.\"\n",
        "        elif 'durban' in input_text.lower():\n",
        "            folium.Marker(location=[-29.8587, 31.0218], popup='Durban').add_to(m)\n",
        "            return \"Added marker for Durban.\"\n",
        "        else:\n",
        "            return \"Location not recognized. Try 'Cape Town' or 'Durban'.\"\n",
        "    return \"Type 'add marker [location]' to place a marker.\"\n",
        "\n",
        "user_input = st.text_input(\"Chat with the map:\")\n",
        "if user_input:\n",
        "    response = handle_chat(user_input)\n",
        "    st.session_state['chat_history'].append((user_input, response))\n",
        "\n",
        "# Show chat history\n",
        "for msg, rsp in st.session_state['chat_history']:\n",
        "    st.markdown(f\"**You:** {msg}\")\n",
        "    st.markdown(f\"**Map:** {rsp}\")\n",
        "\n",
        "# Show updated map\n",
        "st_folium(m, width=700, height=500)\n"
      ]
    },
